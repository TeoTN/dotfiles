    email = staniowp@gmail.com
[push]
    default = simple
[core]
    autocrlf = false
[alias]
    co = checkout
    ci = commit
    st = status
    sh = push
    pu = pull
    br = branch
    me = merge
    fe = fetch
    pick = cherry-pick
    bi = bisect
    rh = reset --hard
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p\n

